body:not(:-moz-handler-blocked) .Header {
    max-width:946px;
  }
.Header
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	z-index: 50;
	background: white;
	margin-top:.5em;
	width: 95%;
	max-width: 990px;
	margin: 0 auto;
	transition: all 0.3s ease-out 0s;
	align-items center
	.Inicio-title
		position fixed
		top 0
		z-index 10001
		width: 95%;
		max-width: 950px;
		margin: 0 auto;
		padding 1.7em
		transition all 0.5s ease
		color transparent
		background transparent
		border-color transparent
	&-scrolled
		top -5em
	&-logos
		display: flex;
		align-items: center;
		justify-content:space-between;
		margin-bottom: 0;
		padding: .5em 1em;
		&-item
			display: flex;
			align-items:center;
			&:last-child
				color $guindo
			img
				height: 4em;
			p
				margin-left: 1em; 

.Header
	&-nav
		ul
			font-size .9em
		.nav-multimediaBoletin
			display flex
			justify-content flex-start
			i
				margin-right 1em
		&-mobile
			display none
		color white
		background $guindo
		ul
			display flex
			justify-content center
			li
				padding 1em
				transition background-color .5s ease
				cursor pointer
				&:hover
					background #681919
				&.sectionActive
					background #681919

@media (max-width:970px)
	.Header
		width 100%
		max-width 100%
		margin 0
@media (max-width:945px)
	.Header
		ul
			li
				text-align center
				display: flex;
				align-items: center;
@media (max-width:873px)
	.Header-nav
		ul
			justify-content: center;
			text-align: center;
			align-items: center;

@media (max-width:800px)
	.Header
		&-logos
			justify-content space-around
			&-item:nth-child(1)
				p
					display none
		&-nav
			ul
				height: 0;
				transition height 0.3s ease
				overflow hidden
				display block !important
				li
					padding 1.3em
					display: flex;
					width: 100%;
			.ul-show
				flex-direction: column;
				align-items: flex-start;
				height 28.5em
				border-top: 1px solid white;
			&-mobile
				display: flex;
				justify-content: flex-end;
				padding: 1em;
				button
					background: none;
					border: none;
				i
					font-size: 1.5rem;

@media (max-width:480px)
	.Header-logos-item
		&:first-child
			display none