.Documentacion
	display flex
	align-items center
	justify-content center
	.Docu
		margin-bottom 2em  
		h2
			text-align center
			display: block;
			width: 100%;
.md-subheader-content
	display flex !important
	flex-direction row
	justify-content space-between
	color black
	p
		br
			display none
	.input
		background-color #ebebeb
		border 0
		border-radius 15px
		color black
		padding: .2em;
.title-programacion
	display flex
.container-delegaciones
	display flex
	flex-direction row
	justify-content space-between
	flex-wrap wrap
	&-items
		width 33.3333333%

.img-tec
	display: flex;
	justify-content: center;

.uls .lis
	list-style circle
.uls
	margin-left 2em
	li
		padding .5em 0
.semblanza
	border 0
	background none
	cursor pointer


@media(max-width 815px)
	.container-delegaciones
		&-items
			width 50%

@media(max-width 668px)
	.container-delegaciones
		justify-content center
		&-items
			width 100%

@media(max-width 500px)
	.md-subheader-content
		flex-direction column
		align-items center
